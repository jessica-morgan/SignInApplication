{
  "name": "sign-in-and-out-app",
  "version": "0.1.0",
  "description": "Sign in/out App for EDA",
  "main": "server/index.js",
  "scripts": {
    "start": "webpack && node server",
    "dev": "nodemon --watch server & webpack --watch",
    "knex": "knex --knexfile server/db/knexfile.js",
    "db:reset": "run-s db:delete db:migrate db:seed",
    "db:migrate": "npm run knex migrate:latest",
    "db:seed": "npm run knex seed:run",
    "db:delete": "rm -f server/db/dev.sqlite3"
  },
  "author": "Jessica Morgan",
  "license": "ISC",
  "babel": {
    "presets": [
      "env",
      "react"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "nodemon": "^1.18.10",
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "react-redux": "^6.0.1",
    "redux": "^4.0.1",
    "webpack": "^3.11.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.2.1"
  },
  "dependencies": {
    "@material-ui/core": "^4.3.1",
    "@material-ui/icons": "^4.5.1",
    "date-fns": "^2.5.0",
    "express": "^4.16.4",
    "express-jwt": "^5.3.1",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^2.2.0",
    "knex": "^0.19.5",
    "libsodium-wrappers-sumo": "^0.7.5",
    "npm-run-all": "^4.1.5",
    "react-router-dom": "^5.0.1",
    "reactn": "^2.2.4",
    "sqlite3": "^4.1.0",
    "superagent": "^5.1.0"
  }
}
